<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class lazy="false"
        name="com.saille.bbs.yssy.BoardRecord" table="BOARDRECORD">
        <id column="OBJID" name="id" type="java.lang.Integer" unsaved-value="0">
            <generator class="servlet.IDGenerator">
                <param name="name">BOARDRECORD</param>
            </generator>
        </id>
        <property column="BOARDNAME" name="boardName" type="java.lang.String"/>
        <property column="AVGTIME" name="avgTime" type="java.lang.Integer"/>
        <property column="TIMES" name="times" type="java.lang.Integer"/>
        <property column="TOTALTIME" name="totalTime" type="java.lang.Double"/>
        <property column="`DATE`" name="date" type="java.lang.Integer"/>
        <property column="`INDEX`" name="index" type="java.lang.Integer"/>
    </class>
</hibernate-mapping>
